{
    "name": "supacons-core",
    "version": "6.1.1",
    "description": "Supacons Core is the backbone of Supacons icons. Font icons ripped from popular Font Awesome 6 icon pack including all Pro icons !",
    "repository": {
        "type": "git",
        "url": "https://github.com/poseidon-code/supacons/tree/core"
    },
    "homepage": "https://supacons.netlify.app",
