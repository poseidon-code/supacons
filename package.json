    "scripts": {
        "start": "sass --watch sass:css",
        "compile": "sass sass:css",
        "build": "sass --no-source-map --style compressed css:dist",
        "clean": "rm -rf css/* dist/*"
    },
    "devDependencies": {
        "sass": "^1.51.0"
    }
}
